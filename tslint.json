{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "semicolon": true,
    "whitespace": false,
    "trailing-comma": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "max-line-length": [
      true,
      180
    ],
    "radix": false,
    "no-string-literal": false
  }
}